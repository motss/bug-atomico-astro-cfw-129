{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsxdev",
    "jsxImportSource": "atomico",
    "noUncheckedSideEffectImports": true,
    "paths": {
      "components:*": ["./src/components/*"],
      "islands:*": ["./src/islands/*"],
      "layouts:*": ["./src/layouts/*"]
    },
    "types": ["@cloudflare/workers-types/2023-07-01"]
  },
  "exclude": [".astro", ".turbo", ".wrangler", "dist"]
}
